# NLP-to-SQL POC Environment Variables
# Copy this file to .env and fill in your values

# Groq API Configuration (Get FREE key from https://console.groq.com/keys)
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama-3.3-70b-versatile

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=your_password_here

# NLP Service Configuration
NLP_SERVICE_HOST=localhost
NLP_SERVICE_PORT=8003
DJANGO_API_URL=http://127.0.0.1:8000

# Rate Limiting (Groq free tier: 30 req/min, 6000 tokens/min)
MAX_REQUESTS_PER_MINUTE=25
MAX_TOKENS_PER_MINUTE=5000
MAX_OUTPUT_TOKENS=256
